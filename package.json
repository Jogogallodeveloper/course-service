  "main": "index.js",
  "scripts": {
    "dev": "node --env-file .env --watch src/server.ts",
    "db:seed": "node --env-file .env src/database/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:test": "dotenv -e .env.test drizzle-kit migrate",
    "db:seed:test": "dotenv -e .env.test tsx src/database/seed.ts",
    "db:seed:dev": "SEED_ALLOW=1 dotenv -e .env tsx src/database/seed.ts",
    "db:studio": "drizzle-kit studio",
    "pretest": "dotenv -e .env.test drizzle-kit migrate",
    "test": "dotenv -e .env.test vitest run --coverage",
        "test:ci": "dotenv -e .env.test drizzle-kit migrate && dotenv -e .env.test vitest run --coverage",
    "env:which:dev": "dotenv -e .env bash -lc 'echo $DATABASE_URL'",
    "env:which:test": "dotenv -e .env.test bash -lc 'echo $DATABASE_URL'"
  },
